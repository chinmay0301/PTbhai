{
  "manifest_version": 2,

  "name": "Happy Birthday!!",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "minions.png",
    "default_popup": "popup.html"

  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"
  ]
  
}


